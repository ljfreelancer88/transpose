{
    "name": "ljfreelancer88/transpose",
    "description": "PHP Transposer",
    "keywords": [
        "ljfreelancer88",
        "transpose"
    ],
    "homepage": "https://github.com/ljfreelancer88/transpose",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jake Pucan",
            "email": "lj88@duck.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Ljfreelancer88\\Transpose\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ljfreelancer88\\Transpose\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "psalm": "vendor/bin/psalm"

    },
    "config": {
        "sort-packages": true
    }
}
